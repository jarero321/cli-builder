{
  "name": "@cjarero183006/cli-builder",
  "version": "1.0.1",
  "description": "Shared UI components and utilities for building beautiful CLI applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "import": "./dist/interfaces/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./configs/tsconfig.base.json": "./configs/tsconfig.base.json",
    "./configs/vitest.base.js": "./configs/vitest.base.js"
  },
  "files": [
    "dist",
    "configs"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "ui",
    "components",
    "prompts",
    "progress",
    "spinner",
    "banner"
  ],
  "author": "Carlos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jarero321/cli-builder.git"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "chalk": "^5.6.2",
    "figlet": "^1.10.0",
    "gradient-string": "^3.0.0"
  }
}
